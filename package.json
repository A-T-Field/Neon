{
    "name": "neon",
    "private": true,
    "version": "1.0.2-0",
    "scripts": {
        "dev": "vite --host",
        "build": "npm run clear && vue-tsc --noEmit && vite build",
        "staging": "npm run clear && vue-tsc --noEmit && vite build --mode staging",
        "preview": "npm run clear && vite preview",
        "clear": "npm cache clean --force && npm cache verify",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui",
        "lint:eslint": "eslint --fix .",
        "lint:prettier": "prettier --write ."
    },
    "dependencies": {
        "axios": "^0.26.1",
        "date-fns": "^2.28.0",
        "lodash-es": "^4.17.21",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.16",
        "ua-parser-js": "^1.0.2",
        "uuid": "^8.3.2",
        "vue": "^3.2.37",
        "vue-router": "^4.1.2"
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.6",
        "@types/nprogress": "^0.2.0",
        "@types/rollup-plugin-visualizer": "^4.2.1",
        "@types/ua-parser-js": "^0.7.36",
        "@types/uuid": "^8.3.4",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
        "@vitest/ui": "^0.9.4",
        "@vue/compiler-sfc": "^3.2.37",
        "@vue/test-utils": "^2.0.2",
        "autoprefixer": "^10.4.7",
        "commitizen": "^4.2.5",
        "cross-env": "^7.0.3",
