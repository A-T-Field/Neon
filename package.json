        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^7.0.4",
        "prettier": "^2.7.1",
        "rollup": "^2.77.0",
        "rollup-plugin-visualizer": "^5.7.1",
        "sass": "^1.53.0",
        "typescript": "^4.7.4",
        "unplugin-auto-import": "^0.7.2",
        "unplugin-vue-components": "^0.19.9",
        "vite": "^2.9.14",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-pages": "^0.23.0",
